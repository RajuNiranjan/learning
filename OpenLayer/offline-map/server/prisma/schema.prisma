generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Tile_Prisma_table {
  id                 Int      @id @default(autoincrement())
  name               String
  extent             Float[]
  center             Float[]
  zoom               Float[]
  projection         String
  thumbnailBase64Img String?  @db.Text
  created_at         DateTime @default(now())
  updated_at         DateTime @updatedAt
}
